<UICollectionViewCell clipsToBounds="true"
    backgroundColor="clear"
    >
    
    <UIView
        id="Container"
        layer.cornerRadius="10.0"
        layer.borderWidth="1.0"
        layer.borderColor="#181818"
        backgroundColor="#151515"
        height="100% - 10"
        top="parent.top + 5"
        leading="15"
        trailing="15">

    <UIView
        id="TopViewHolder"
        height="50% - 20"
        top="parent.top + 20"
        leading="20"
        trailing="20">
        
        <UIImageView
            id="ProfileImage"
            contentMode="scaleAspectFit"
            top="parent.top"
            bottom="parent.bottom"
            width="height"
            leading="10"
            image="profileUser"
            layer.cornerRadius="width/2"
            clipsToBounds="true"/>
            
            <UIView
                height="parent.height"
                top="parent.top"
                leading="previous.right + 20"
                width="90% - 30">
                
                <UILabel
                    font="HelveticaNeue-Medium 16"
                    text="{name}"
                    textColor="UIColor.white"
                    textAlignment="left"
                    height="50%"
                    top="parent.top"
                    leading="parent.left"
                    trailing="parent.trailing"/>
                
                <UILabel
                    font="HelveticaNeue 12"
                    text="{profession}"
                    textColor="UIColor.lightGray"
                    textAlignment="left"
                    height="50%"
                    bottom="parent.bottom"
                    leading="parent.left"
                    trailing="parent.trailing"/>
            </UIView>
    </UIView>
    
    
    <UIView
        id="BottomViewHolder"
        height="50% - 25"
        bottom="parent.bottom - 20"
        leading="20"
        trailing="20">
        
        <UIView
            id="GenderView"
            width="100% / 3"
            top="parent.top"
            leading="parent.left"
            bottom="parent.bottom">
            
            <UILabel
            font="HelveticaNeue 10"
            text="Gender:"
            textColor="UIColor.lightGray"
            textAlignment="left"
            height="50%"
            top="parent.top"
            leading="parent.left"
            trailing="parent.trailing > 5"/>
            
            <UILabel
            font="HelveticaNeue-Medium 16"
            text="{gender}"
            textColor="UIColor.white"
            textAlignment="left"
            height="50%"
            bottom="parent.bottom"
            leading="parent.left"
            trailing="parent.trailing > 5"/>
        </UIView>
        
        <UIView
            id="Nationality"
            width="100% / 3"
            top="parent.top"
            leading="previous.trailing"
            bottom="parent.bottom">
            
            <UILabel
            font="HelveticaNeue 10"
            text="Nationality:"
            textColor="UIColor.lightGray"
            textAlignment="left"
            height="50%"
            top="parent.top"
            leading="parent.left"
            trailing="parent.trailing > 5"/>
            
            <UILabel
            font="HelveticaNeue-Medium 16"
            text="{nationality}"
            textColor="UIColor.white"
            textAlignment="left"
            height="50%"
            bottom="parent.bottom"
            leading="parent.left"
            trailing="parent.trailing > 5"/>

        </UIView>

        <UIView
            id="Phone"
            width="100% / 3"
            top="parent.top"
            leading="previous.trailing"
            bottom="parent.bottom">
            
            <UILabel
            font="HelveticaNeue 10"
            text="Phone:"
            textColor="UIColor.lightGray"
            textAlignment="left"
            height="50%"
            top="parent.top"
            leading="parent.left"
            trailing="parent.trailing > 5"/>
            
            <UILabel
            font="HelveticaNeue-Medium 16"
            text="{phone}"
            textColor="UIColor.white"
            textAlignment="left"
            height="50%"
            bottom="parent.bottom"
            leading="parent.left"
            trailing="parent.trailing > 5"/>

        </UIView>
    </UIView>
    </UIView>
</UICollectionViewCell>


